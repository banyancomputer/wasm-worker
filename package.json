{
  "name": "wasm-hasher",
  "version": "0.0.0",
  "description": "Simple example of consuming a Wasm Web Worker with WebPack",
  "scripts": {
    "build": "webpack",
    "build:wasm-mt": "cross-env RUSTFLAGS=\"-C target-feature=+atomics,+bulk-memory,+mutable-globals\" rustup run nightly wasm-pack build --target web",
    "dev": "webpack-dev-server",
    "test": "wasm-pack test --firefox --headless"
  },
  "dependencies": {
    "comlink": "^4.4.1",
    "cross-env": "^7.0.3",
    "crossenv": "^0.0.2-security"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banyancomputer/wasm-worker.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Al Miller <alex@banyan.computer>",
  "license": "(MIT)"
}
